{
  "name": "ascent-formation-refonte",
  "version": "1.0.0",
  "description": "Refonte de la page d'accueil Ascent Formation - Exercice Full-Stack",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "node --watch backend/src/index.js",
    "dev:frontend": "cd frontend && npm run dev",
    "start": "node backend/src/index.js",
    "test": "jest --runInBand",
    "test:backend": "jest --runInBand --testPathPattern=backend",
    "test:frontend": "cd frontend && npm test"
  },
  "keywords": [
    "formation",
    "refonte",
    "fullstack",
    "api"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "mock-fs": "^5.2.1",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^6.0.0",
    "@testing-library/user-event": "^14.4.3",
    "react-test-renderer": "^18.2.0",
    "jsdom": "^22.1.0",
    "jest-environment-jsdom": "^29.7.0",
    "esbuild": "^0.18.20",
    "esbuild-jest": "^0.5.0"
  }
}
